unit DTO.Caixa;

interface

type
  TCaixa = class
  private
    FFechamento: TDateTime;
    FTotal: Currency;
    FAbertura: TDateTime;
    procedure SetAbertura(const Value: TDateTime);
    procedure SetFechamento(const Value: TDateTime);
    procedure SetTotal(const Value: Currency);
  published
    property Abertura: TDateTime read FAbertura write SetAbertura;
    property Fechamento: TDateTime read FFechamento write SetFechamento;
    property Total: Currency read FTotal write SetTotal;
  end;

implementation

{ TCaixa }

procedure TCaixa.SetAbertura(const Value: TDateTime);
begin
  FAbertura := Value;
end;

procedure TCaixa.SetFechamento(const Value: TDateTime);
begin
  FFechamento := Value;
end;

procedure TCaixa.SetTotal(const Value: Currency);
begin
  FTotal := Value;
end;

end.
